{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "target": "es2020",
        "baseUrl": "./",
        "paths": {
            "@middleware/*": ["src/middleware/*"],
            "@utils/*": ["src/utils/*"],
            "@config/*": ["src/config/*"],
            "@controllers/*": ["src/controllers/*"],
            "@services/*": ["src/services/implementations/*"]
        }
    },
    "minify": false,
    "sourceMaps": true,
    "module": {
        "type": "commonjs"
    }
}
